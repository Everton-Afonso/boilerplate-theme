  "slider-layout#promotion-images": {
    "props": {
      "blockClass": "promotion",
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true
    }
  },

  "tab-content": {
    "children": ["tab-content.item#1", "tab-content.item#2"]
  },
  "tab-content.item#1": {
    "props": {
      "tabId": "promocao"
    },
    "children": ["rich-text#1"]
  },
  "rich-text#1": {
    "props": {
      "text": "Texto para Major Appliances",
      "textPosition": "CENTER",
      "font": "t-heading-3"
    }
  },
  "tab-content.item#2": {
    "props": {
      "tabId": "corinthians"
    },
    "children": ["rich-text#2"]
  },
  "rich-text#2": {
    "props": {
      "text": "Texto para Electronics",
      "textPosition": "CENTER",
      "font": "t-heading-3"
    }
  },
  "list-context.product-list": {
    "blocks": ["product-summary.shelf#products"],
    "children": ["slider-layout"],
    "props": {
      "maxItems": 4,
      "category": "20",
      "hideUnavailableItems": true,
      "blockClass": "products"
    }
  },
  "slider-layout": {
    "props": {
      "blockClass": "products-layout"
    }
  },
  "product-summary.shelf#products": {
    "children": [
      "modal-trigger#quickview",
      "product-summary-name",
      "product-rating-inline",
      "product-summary-space",
      "product-summary-price",
      "product-summary-buy-button"
    ]
  },
  "modal-trigger#quickview": {
    "children": ["product-summary-image", "modal-layout#quickview"]
  },
  "modal-layout#quickview": {
    "children": [
      "product-summary-name",
      "product-images",
      "product-summary-sku-selector",
      "product-summary-quantity",
      "add-to-cart-button"
    ]
  }
}
