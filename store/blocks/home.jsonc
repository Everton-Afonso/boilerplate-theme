{
  "store.home": {
    "blocks": ["tab-layout#home", "list-context.product-list"]
  },
  "tab-layout#home": {
    "children": [
      "tab-list#home",
      "list-context.image-list#promotion",
      "tab-content"
    ]
  },
  "tab-list#home": {
    "children": ["slider-layout#layout-menu"]
  },
  "slider-layout#layout-menu": {
    "props": {
      "itemsPerPage": {
        "desktop": 7,
        "tablet": 4,
        "phone": 3
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "menu-layout"
    },
    "children": [
      "tab-list.item#1",
      "tab-list.item#2",
      "tab-list.item#3",
      "tab-list.item#4",
      "tab-list.item#5",
      "tab-list.item#6",
      "tab-list.item#7",
      "tab-list.item#8",
      "tab-list.item#9",
      "tab-list.item#10"
    ]
  },
  "tab-list.item#1": {
    "props": {
      "tabId": "promocao",
      "label": "PROMOÇÃO",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#2": {
    "props": {
      "tabId": "corinthians",
      "label": "CORINTHIANS",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#3": {
    "props": {
      "tabId": "palmeiras",
      "label": "PALMEIRAS",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#4": {
    "props": {
      "tabId": "signos",
      "label": "SIGNOS",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#5": {
    "props": {
      "tabId": "divertidos",
      "label": "DIVERTIDOS",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#6": {
    "props": {
      "tabId": "maravilhas",
      "label": "7 MARAVILHAS",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#7": {
    "props": {
      "tabId": "guerreiros",
      "label": "GUERREIROS",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#8": {
    "props": {
      "tabId": "album1",
      "label": "ÁLBUM 1",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#9": {
    "props": {
